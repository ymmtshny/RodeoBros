		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/vue/1.0.11/vue.min.js")
		script.
			
			jQuery(function($) {
				var nav = $('#fixedBox');
				offset = nav.offset();

				$(window).scroll(function () {
					if($(window).scrollTop() > offset.top) {
					
					$("#fixedBox").css({
						position: "fixed",
						top: "100px",
						width:"100%"
						});

					} else {
						$("#fixedBox").css({
							position: "",
							top: ""
						});
						
					}
				});

				});
		script.
			new Vue({
				el: '#Vue',
				//template: '#VueTopsTemplate',

				data: {
					Souvenir: []
				},
				
				created: function() {
					
					var _this = this;
					//get json file
					$.ajax({
						url: './json/souvenirjacket.json',
						dataType: 'json'
					}).done(function(data) {
						var temp = [];
						
						data.forEach(function (el) {
							el.number = "http://store.shopping.yahoo.co.jp/rodeobros/" + el.number + ".html";
							el.image = "http://thumbnail.image.rakuten.co.jp/@0_mall/auc-rodeo/cabinet/" + el.image + "-0.jpg?_ex=700x700";
							temp.push(el);
						});
						_this.Souvenir = temp;
						 console.log(_this.items); 
					});

				}
			});



