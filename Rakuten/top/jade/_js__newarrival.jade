script.

		$('.flexslider').flexslider({
			animation: "fade",
			slideshow: false,
			slideshowSpeed: 3000
		});

		//- $('.newArrival__item ').unwrap();
		//- $('.newArrival__item').unwrap();


		var lastWidth = $(window).width();
		$(window).resize(function(){
			if($(window).width()!=lastWidth){
				//execute code.
				lastWidth = $(window).width();
				console.log(lastWidth);
			}

			if(lastWidth < 768) {

			}


		});

		var currentpage = 1;
		var NUMBER_OF_ITMES = 12;
		var vm = new Vue({
			el: '#Vue__NewArrival',
			//template: '#VueTopsTemplate',

			methods: {
				onClickNext: function (e) {
					
					switch(currentpage) {
						case 1:
								vm.$set('PageOne', this.PageTwo);
								currentpage = 2;
								break;
						case 2:
								vm.$set('PageOne', this.PageThree);
								currentpage = 3;
								break;
						case 3:
								vm.$set('PageOne', this.PageFour);
								currentpage = 4;
								break;
						case 4:
								vm.$set('PageOne', this.PageOne);
								currentpage = 1;
								break;
						default:
								console.log("ERROR");
					}

					setTimeout(function(){ 
							LazyLoad();
					},100);

					console.log(currentpage);

				},

				onClickBack: function (e) {
					switch(currentpage) {
						case 1:
								vm.$set('PageOne', this.PageFour);
								currentpage = 4;
								break;
						case 2:
								vm.$set('PageOne', this.PageOne);
								currentpage = 1;
								break;
						case 3:
								vm.$set('PageOne', this.PageTwo);
								currentpage = 2;
								break;
						case 4:
								vm.$set('PageOne', this.PageThree);
								currentpage = 3;
								break;
						default:
								console.log("ERROR");
					}

					setTimeout(function(){ 
							LazyLoad();
					},200);

					console.log(currentpage);

				}

			},

			data: {
				NewArrival: [],
				PageOne:[],
				PageTwo:[],
				PageThree:[],
				PageFour:[]
			},
			
			created: function() {
				
				var _this = this;
				//get json file
				$.ajax({
					//async:false,//同期処理にする。
					url: './data/new-arrival.json',
					dataType: 'json'
				}).done(function(data) {
					var temp = [];
					
					data.forEach(function (el) {
						el.number = "http://item.rakuten.co.jp/auc-rodeo/" + el.number + "/";
						el.image = "http://thumbnail.image.rakuten.co.jp/@0_mall/auc-rodeo/cabinet/" + el.image + "-0.jpg?_ex=500x500";
						temp.push(el);
					});

					 _this.NewArrival = temp.slice(0,　25);
					 _this.PageOne = temp.slice(0,NUMBER_OF_ITMES);
					 _this.PageTwo = temp.slice(NUMBER_OF_ITMES + 1, NUMBER_OF_ITMES * 2 + 1);
					 _this.PageThree = temp.slice(NUMBER_OF_ITMES * 2 + 1, NUMBER_OF_ITMES * 3 + 1);
					 _this.PageFour = temp.slice(NUMBER_OF_ITMES * 3 + 1, NUMBER_OF_ITMES * 4 + 1);
					//console.log(_this.NewArrival); 
				});

			}

		});

			function LazyLoad() {
				$("img.lazy").lazyload({
							"threshold":250,
							"effect":"fadeIn",
							"effect_speed":800,
							"failure_limit":1} );
						console.log("Lazy Loadを起動する");

			}
	
			// Lazy Loadを起動する
			setTimeout(function(){ 
				LazyLoad();
			},100);
