script.

		new Vue({
			el: '#Vue',
			//template: '#VueTopsTemplate',

			data: {
				totalCoorde: [],
				NewArrival: []
			},
			
			created: function() {
				
				var _this = this;
				//get json file
				$.ajax({
					//async:false,//同期処理にする。
					url: './data/total-coordinate.json',
					dataType: 'json'
				}).done(function(data) {
					var temp = [];
					
					data.forEach(function (el) {
						el.number = "http://item.rakuten.co.jp/auc-rodeo/" + el.number + "/";
						el.image = "http://thumbnail.image.rakuten.co.jp/@0_mall/auc-rodeo/cabinet/" + el.image + "-0.jpg?_ex=500x500";
						temp.push(el);
					});
					_this.totalCoorde = temp;
					 console.log(_this.totalCoorde); 
				});

				$.ajax({
					//async:false,//同期処理にする。
					url: './data/new-arrival.json',
					dataType: 'json'
				}).done(function(data) {
					var temp = [];
					
					data.forEach(function (el) {
						el.number = "http://item.rakuten.co.jp/auc-rodeo/" + el.number + "/";
						el.image = "http://thumbnail.image.rakuten.co.jp/@0_mall/auc-rodeo/cabinet/" + el.image + "-0.jpg?_ex=500x500";
						temp.push(el);
					});
					_this.NewArrival = temp;
					 console.log(_this.NewArrival); 
				});

			}

		});

		//ajaxを同期処理しているので以下は、vue.jsの処理後に発動する。
			// Lazy Loadを起動する
			$(function(){
				$("img.lazy").lazyload({
					"threshold":250,
					"effect":"fadeIn",
					"effect_speed":800,
					"failure_limit":1} );
				console.log("Lazy Loadを起動する");
			});

